{
  "name": "eslint-plugin-square",
  "version": "13.0.0",
  "description": "Linting plugin containing Square's custom lint rules and lint configuration for JavaScript and Ember.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "npm-run-all lint:* --continue-on-error",
    "lint:docs": "markdownlint '**/*.md' -i CHANGELOG.md -i node_modules",
    "lint:js": "eslint .",
    "preversion": "yarn test && yarn lint",
    "test": "nyc --all --check-coverage mocha --recursive tests"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ember",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "linter"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/square/eslint-plugin-square.git"
  },
  "engines": {
    "node": ">= 10"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-ember": "^7.11.1",
    "eslint-plugin-es": "^3.0.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "espree": "^6.2.1",
    "estraverse": "^5.0.0",
    "prettier": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-plugin-eslint-plugin": "2.2.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-unicorn": "18.0.1",
    "markdownlint-cli": "0.22.0",
    "mocha": "7.1.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "strip-indent": "3.0.0"
  },
  "nyc": {
    "branches": 93,
    "lines": 96,
    "functions": 97,
    "statements": 96
  }
}
